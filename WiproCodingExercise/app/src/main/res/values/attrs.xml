<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="RobotoTextView">
        <attr name="typeface" />
    </declare-styleable>
    <declare-styleable name="RobotoEditText">
        <attr name="typeface" />
    </declare-styleable>
    <declare-styleable name="RobotoButton">
        <attr name="typeface" />
    </declare-styleable>


    <declare-styleable name="BaseTheme">
        <attr name="actionBarIconColor" format="color" />
        <attr name="actionBarInsetStart" format="dimension" />
        <attr name="spinnerBarInsetStart" format="dimension" />
        <attr name="popupItemBackground" format="reference" />
        <attr name="photoItemForeground" format="reference" />
        <attr name="photoItemForegroundBorderless" format="reference" />
    </declare-styleable>

    <attr name="typeface" format="enum">
        <enum name="roboto_regular" value="0" />
        <enum name="roboto_bold" value="1" />
        <enum name="roboto_boldCond" value="2" />
        <enum name="roboto_boldcontit" value="3" />
        <enum name="roboto_boldit" value="4" />
        <enum name="roboto_cond" value="5" />
        <enum name="roboto_condit" value="6" />
        <enum name="roboto_it" value="7" />
        <enum name="roboto_light" value="8" />
        <enum name="roboto_semibold" value="9" />
        <enum name="roboto_semiboldit" value="10" />
    </attr>

    <declare-styleable name="ExpandableTextView">
        <attr name="trimLength" format="integer"/>
    </declare-styleable>

</resources>